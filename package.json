{
    "name": "@bludevkit/vue-ui",
    "version": "0.0.2",
    "author": {
        "name": "Ymaro Blue",
        "email": "coding.ymaro@gmail.com"
    },
    "description": "Blu Dev Kit is a UI library made with Vue 3 and Tailwindcss",
    "keywords": [
        "vue",
        "vue3",
        "vuejs",
        "Nuxt",
        "Nuxt3",
        "component",
        "library",
        "vue-components",
        "ui",
        "tailwind",
        "tailwindcss",
        "typescript"
    ],
    "type": "module",
    "types": "./index.d.ts",
    "main": "dist/blu-dev-kit",
    "files": [
        "dist",
        "index.d.ts"
    ],
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BluDevKit/vue-ui.git"
    },
    "scripts": {
        "dev": "vite",
        "build": "vue-tsc --noEmit && vite build",
        "build:all": "npm run build && npm run storybook:build",
        "preview": "vite preview",
        "storybook": "storybook dev -p 6006",
        "storybook:serve-build": "npx http-server .storybook/build",
        "storybook:build": "storybook build -o .storybook/build && xcopy /Y .storybook\\build dist\\storybook",
        "storybook:build:serve": "npm run storybook:build && npm run storybook:serve-build",
        "release": "standard-version && git push --follow-tags && npm publish",
        "test": "echo \"Error: no test specified\""
    },
    "peerDependencies": {
        "vue": "^3.4.15"
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^7.6.16",
        "@storybook/addon-interactions": "^7.6.16",
        "@storybook/addon-links": "^7.6.16",
        "@storybook/addon-onboarding": "^1.0.11",
        "@storybook/blocks": "^7.6.16",
        "@storybook/test": "^7.6.16",
        "@storybook/testing-library": "^0.2.2",
        "@storybook/vue3": "^7.6.16",
        "@storybook/vue3-vite": "^7.6.16",
        "@tailwindcss/forms": "^0.5.7",
        "@types/node": "^20.11.19",
        "@vitejs/plugin-vue": "^5.0.3",
        "@vue/test-utils": "^2.4.4",
        "autoprefixer": "^10.4.17",
        "http-server": "^14.1.1",
        "postcss": "^8.4.35",
        "postcss-cli": "^11.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.71.0",
        "standard-version": "^9.5.0",
        "storybook": "^7.6.16",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.2.2",
        "vite": "^5.1.0",
        "vite-plugin-css-injected-by-js": "^3.4.0",
        "vite-plugin-dts": "^3.7.2",
        "vite-plugin-lib-inject-css": "^1.3.0",
        "vitest": "^1.3.1",
        "vue-tsc": "^1.8.27"
    }
}
